{
    "run_name": "Run CIFAR10",
    "dataset": "CIFAR10",
    "federated_setup": {
        "aggregator": "FedOpt",
        "global_learning_rate": 1.0,
        "local_optimizer": "SGD",
        "local_learning_rate": 0.01,
        "batch_size": 64,
        "iterations": 80,
        "local_epochs_per_iteration": 3,
        "scheduler": false
    },
    "model": {
        "architecture": "ResNet34 (Resized)",
        "pretrained": false
    },
    "sattler": {
        "eps1": 185.00,
        "eps2": 925.00,
        "round_cooldown": 20
    },
    "briggs": {
        "distance_threshold": 0.05,
        "clustering_round": 20
    }
}
