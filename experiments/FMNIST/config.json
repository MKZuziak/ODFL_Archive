{
    "run_name": "Run I (FMNIST)",
    "dataset": "FMNIST",
    "federated_setup": {
        "aggregator": "FedOpt",
        "global_learning_rate": 1.0,
        "local_optimizer": "SGD",
        "local_learning_rate": 0.01,
        "batch_size": 32,
        "iterations": 50,
        "local_epochs_per_iteration": 3,
        "scheduler": false
    },
    "model": {
        "architecture": "Fmnist Expanded CNN (custom)",
        "pretrained": false
    },
    "sattler": {
        "eps1": 25,
        "eps2": 125,
        "round_cooldown": 40
    },
    "briggs": {
        "distance_threshold": 0.05,
        "clustering_round": 20
    },
    "notes": ""
}
